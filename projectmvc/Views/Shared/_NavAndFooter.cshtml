<body class="">

    <header class="header-area header-sticky mb-5 overflow-hidden">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <nav class="main-nav">
                        <!-- ***** Logo Start ***** -->
                        <a href="/Category/index" class="logo mt-3">
                            <img src="/assets/images/logo.png">
                        </a>
                        <!-- ***** Logo End ***** -->
                        <!-- ***** Menu Start ***** -->
                        <ul class="nav">
                            <li class="scroll-to-section"><a href="#top" class="active">@ViewBag.Title</a></li>
                          
                           
                            <li class="scroll-to-section"><a href="#">Hello @ViewBag.UserName</a></li>
                               @if (User.IsInRole("Customer"))
                                 {


                                <li class="scroll-to-section"><a href="/category/GetAllProductByCategory/1">Men</a></li>
                                <li class="scroll-to-section"><a href="/category/GetAllProductByCategory/2">Women</a></li>
                                <li class="scroll-to-section"><a href="/category/GetAllProductByCategory/3">Kid</a></li>
                                <a class=" btn btn-outline-secondary" href="/cart/AllCustomerProduct">
                                <i class="fa-solid fa-cart-shopping pt-1"></i> </a>
                                 
                            }

                            @if (User.Identity.IsAuthenticated)
                            {
                                <li class="scroll-to-section"><a href="/Account/signOut">SignOut</a></li>
                            }
                        </ul>
                        <a class='menu-trigger'>
                            <span>Menu</span>
                            
                        </a>
                        <!-- ***** Menu End ***** -->
